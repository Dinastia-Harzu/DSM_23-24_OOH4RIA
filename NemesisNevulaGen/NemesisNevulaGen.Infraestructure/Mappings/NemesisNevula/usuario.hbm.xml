<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="NemesisNevulaGen.Infraestructure"
		                   namespace="NemesisNevulaGen.Infraestructure.EN.NemesisNevula">
		    
		    
			
				
