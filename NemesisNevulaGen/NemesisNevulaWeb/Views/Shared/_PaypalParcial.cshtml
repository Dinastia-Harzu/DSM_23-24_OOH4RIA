@model IEnumerable<NemesisNevulaWeb.Models.PaypalVM>

<p>
    @Html.ActionLink("Crear Paypal", "Create", "Paypal")
</p>
<table class="table text-white mt-1">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Pass)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Pass)
            </td>
            <td>
                    @Html.ActionLink("Editar", "Edit", "Paypal", new { id = item.Id }) |
                    @Html.ActionLink("Detalles", "Details", "Paypal", new { id = item.Id }) |
                    @Html.ActionLink("Eliminar", "Delete", "Paypal", new { id = item.Id })
            </td>
        </tr>
}
    </tbody>
</table>
